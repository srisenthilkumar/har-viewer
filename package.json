{
	"name": "har-viewer",
	"displayName": "HAR Viewer",
	"description": "HTTP Archive Viewer",
	"version": "0.0.1",
	"publisher": "unclebeast",
	"engines": {
		"vscode": "^1.23.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:har-viewer.preview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "har-viewer.preview",
				"title": "Preview",
				"category": "HAR Viewer"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/jsesc": "^2.5.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"jsesc": "^3.0.1"
	}
}
